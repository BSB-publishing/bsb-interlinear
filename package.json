{
  "name": "bsb-interlinear",
  "version": "1.0.0",
  "description": "A mobile-first web app for exploring the Berean Standard Bible with Strong's concordance",
  "type": "module",
  "author": "Lars Gunnarson",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/larsgson/bsb-interlinear"
  },
  "keywords": [
    "bible",
    "berean-standard-bible",
    "strongs-concordance",
    "interlinear",
    "hebrew",
    "greek",
    "bible-study"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.10.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9",
    "prettier": "^3",
    "typescript": "~5.6.0",
    "vite": "^6.0.0"
  }
}
